@model WebApplication5.Models.Card
<div class="col-xl-3 col-lg-4 col-md-6 col-sm-12">
    <div class="card">
        <div class="headshot">
            <img class="card-img-top" src="~/image/@Model.Photo" alt="" />
        </div>
        <div class="card-block">
            <h3 class="card-title">@Model.Name</h3><br />
            <p class="card-text">@Model.Brief</p>
            <a href="@Model.WikiUrl" class="btn btn-primary">@Model.Name </a>
        </div>
    </div>
</div>


